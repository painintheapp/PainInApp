{
  "expo": {
    "name": "PainLes",
    "slug": "painles",
    "scheme": "painles",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "android": {
      "versionCode": 1,
      "splash": {
        "mdpi": "./assets/splash.png",
        "hdpi": "./assets/splash.png",
        "xhdpi": "./assets/splash.png",
        "xxhdpi": "./assets/splash.png",
        "xxxhdpi": "./assets/splash.png",
        "resizeMode": "cover"
      },
      "package": "com.PainLes.app"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "0.1.0",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.PainLes.app"
    }
  }
}
